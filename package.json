{
  "name": "marvel-react-app",
  "version": "1.0.0",
  "description": "Marvel React App",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "dev": "cross-env NODE_ENV=development npm start",
    "start": "nodemon server.js --exec babel-node NODE_ENV=development",
    "prod": "cross-env NODE_ENV=production npm run start:prod",
    "start:prod": "npm run build && nodemon server.js --exec babel-node NODE_ENV=production",
    "lint": "eslint src",
    "test:backend": "./node_modules/.bin/mocha --compilers js:babel-core/register server/**",
    "test": "jest --config ./client/__tests__/jest.config.js",
    "test:watch": "jest --watch --config ./client/__tests__/jest.config.js",
    "test:coverage": "jest --coverage --config ./client/__tests__/jest.config.js",
    "test:update": "jest --config ./client/__tests__/jest.config.js --updateSnapshot"
  },
  "license": "MIT",
  "devDependencies": {
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.0",
    "jest": "22.1.4",
    "moxios": "0.4.0",
    "react-test-renderer": "16.2.0",
    "redux-mock-store": "1.4.0"
  },
  "dependencies": {
    "autoprefixer": "6.3.1",
    "axios": "0.14.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "22.1.0",
    "babel-loader": "6.2.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "body-parser": "1.14.1",
    "chai": "3.5.0",
    "chart.js": "2.7.1",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.2.0",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.3",
    "mocha": "3.1.2",
    "mongoose": "4.2.5",
    "morgan": "1.7.0",
    "node-sass": "3.7.0",
    "nodemon": "1.11.0",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-chartjs-2": "2.7.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "rimraf": "2.4.3",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.0",
    "supertest": "2.0.1",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
